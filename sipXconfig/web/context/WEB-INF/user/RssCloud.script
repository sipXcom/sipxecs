  <!DOCTYPE script PUBLIC
  "-//Apache Software Foundation//Tapestry Script Specification 3.0//EN"
  "http://jakarta.apache.org/tapestry/dtd/Script_3_0.dtd">
  <script>
  <include-script resource-path="/org/sipfoundry/sipxconfig/js/jquery-1.4.2.min.js"/>
  <body>
    <![CDATA[
this.loadRSS = function(){
	    $.get('https://sipxcom.org/feed/', function (data) {
var content = "";
    $(data).find("item").each(function () { // or "item" or whatever suits your feed
        var el = $(this);
		content = content + "<h4><a href='"+el.find("link").text() + "' target='_blank'>" + el.find("title").text() + "</a></h4>";
    });
	var theDiv = document.getElementById("rssDiv");
	theDiv.innerHTML = content;
});
}
    ]]>
  </body>
  <initialization>
    jQuery(document).ready(function() {
    	loadRSS();
	});
  </initialization>
</script>
