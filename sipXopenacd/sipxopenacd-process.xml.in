<?xml version='1.0' encoding='iso-8859-1' standalone='yes'?>
<sipXecs-process xmlns='http://www.sipfoundry.org/sipX/schema/xml/sipXecs-process-01-00'>
  <name>OpenACDServer</name>
  <version>@PACKAGE_VERSION@</version>
  <commands>
    <configtest>
      <execute>@SIPX_BIN_DIR@/sipxopenacd</execute>
      <parameter>--configtest</parameter>
    </configtest>
    <start>
      <execute>@SIPX_BIN_DIR@/sipxopenacd</execute>
      <parameter>--start</parameter>
    </start>
    <stop>
      <execute>@SIPX_BIN_DIR@/sipxopenacd</execute>
      <parameter>--stop</parameter>
    </stop>
  </commands>
  <status>
    <pid>@SIPX_RUN_DIR@/sipxopenacd.pid</pid>
    <log>@SIPX_LOG_DIR@/sipxopenacd.log</log>
  </status>
  <resources>
    <osconfig>@SIPX_CONF_DIR@/domain-config</osconfig>
  </resources>
</sipXecs-process>
